<form method="post" action="{{ path('app_thematic_delete', {'id': thematic.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette thématique ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ thematic.id) }}">
    <button class="btn text-secondary" style="font-size: 20px;"><img class="" src="">Supprimer</button>
</form>
